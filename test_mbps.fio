[global]
ioengine=posixaio
size=10g
end_fsync=1
randrepeat=1
gtod_reduce=1
bs=4m
iodepth=1

[tp-seq-read-buf]
rw=read
buffered=1
wait_for_previous

[tp-seq-read-unbuf]
rw=read
buffered=0
wait_for_previous

[tp-seq-write-buf]
rw=write
buffered=1
wait_for_previous

[tp-seq-write-unbuf]
rw=write
buffered=0
wait_for_previous

[tp-seq-rw-buf]
rw=rw
buffered=1
wait_for_previous

[tp-seq-rw-unbuf]
rw=rw
buffered=0
wait_for_previous
