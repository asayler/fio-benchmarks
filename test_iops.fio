[global]
ioengine=posixaio
size=1g
end_fsync=1
randrepeat=1
gtod_reduce=1
bs=4k
iodepth=64

[iops-rand-read-buf]
rw=randread
buffered=1
wait_for_previous

[iops-rand-read-unbuf]
rw=randread
buffered=0
wait_for_previous

[iops-rand-write-buf]
rw=randwrite
buffered=1
wait_for_previous

[iops-rand-write-unbuf]
rw=randwrite
buffered=0
wait_for_previous

[iops-rand-rw-buf]
rw=randrw
buffered=1
wait_for_previous

[iops-rand-rw-unbuf]
rw=randrw
buffered=0
wait_for_previous
